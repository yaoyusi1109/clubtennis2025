# Load the data
data <- read.table("FMS_data.txt", header = TRUE, sep = "\t")
# Remove rows with missing values in either Pre.weight or Pre.height
clean_data <- data[!is.na(data$Pre.weight) & !is.na(data$Pre.height), ]
# Calculate means
weight_mean <- mean(clean_data$Pre.weight)
height_mean <- mean(clean_data$Pre.height)
print(paste("Mean weight:", round(weight_mean, 2)))
print(paste("Mean height:", round(height_mean, 2)))
# Create groups based on means
weight_group <- ifelse(clean_data$Pre.weight <= weight_mean, "Low", "High")
height_group <- ifelse(clean_data$Pre.height <= height_mean, "Low", "High")
# Make contingency table
contingency_table <- table(weight_group, height_group)
print("Contingency Table:")
print(contingency_table)
# Chi-square test
test_result <- chisq.test(contingency_table)
print(paste("Chi-square statistic:", round(test_result$statistic, 4)))
print(paste("P-value:", round(test_result$p.value, 6)))
# Conclusion
if(test_result$p.value < 0.05) {
print("Conclusion: Reject null hypothesis. Pre.weight and Pre.height are NOT independent.")
} else {
print("Conclusion: Do not reject null hypothesis. No evidence that variables are dependent.")
}
